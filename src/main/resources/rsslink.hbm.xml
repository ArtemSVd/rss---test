<?xml version='1.0'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gui.model">

    <class name="RssLink" table="rss_link">
        <id name="id" type="int" column="id" >
            <generator class="increment" />
        </id>

        <property name="name" >
            <column name="name" />
        </property>
        <property name="url">
            <column name="url"/>
        </property>
    </class>

</hibernate-mapping>